    "main": "server.js",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"nodemon server.js\" \"cd frontend && ng serve\" \"cd frontend/projects/music && ng serve --port=4201\"",
        "study": "concurrently \"cd frontend && ng serve --host 0.0.0.0 \"",
        "music": "concurrently \"cd frontend/projects/music && ng serve --host 0.0.0.0 --port=4201\"",
        "study:music": "concurrently \"cd frontend && ng serve\" \"cd frontend/projects/music && ng serve --port=4201\"",
        "backend": "nodemon server.js",
        "serve": "node server.js",
        "build:music": "cd frontend/projects/music && ng build --base-href=/music/ --deploy-url=/music/ --configuration production",
        "build:frontend": "cd frontend && ng build --configuration production",
        "build:all": "npm run build:frontend && npm run build:music",
        "build:all:run": "npm run update:music && npm run build:all && npm run serve",
        "update:music": "node update_version.js"
    },
